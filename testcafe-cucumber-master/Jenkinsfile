pipeline {
  agent any
 
  tools {nodejs "recent_node"}
 
  stages {
    stage('Build') {
        steps {   
            sh 'npm install'
            sh 'npm config ls'   
        }
    }    
    stage('Clonning Git') {
        steps {
            git 'https://github.com/ivan-molina/TFM_CUCUMBER.git'
        }
    }
    
    stage('Install dependencies') {
        steps {
            sh 'npm install testCafe'
        }
        
    }
    
  }
}